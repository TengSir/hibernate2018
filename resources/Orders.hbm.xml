<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.oracle.model.Orders" table="orders" schema="hibernate">
        <id name="orderid">
            <column name="orderid" sql-type="int(11)"/>
        </id>
        <property name="times">
            <column name="times" sql-type="datetime"/>
        </property>
        <property name="addressid">
            <column name="addressid" sql-type="int(11)" not-null="true"/>
        </property>
        <property name="status">
            <column name="status" sql-type="int(11)"/>
        </property>
        <property name="userid">
            <column name="userid" sql-type="int(11)" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>