<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.oracle.model.Orderitems" table="orderitems" schema="hibernate">
        <id name="itemsid">
            <column name="itemsid" sql-type="int(11)"/>
        </id>
        <property name="productid">
            <column name="productid" sql-type="int(11)" not-null="true"/>
        </property>
        <property name="count">
            <column name="count" sql-type="int(11)"/>
        </property>
        <property name="price">
            <column name="price" sql-type="float" precision="-1"/>
        </property>
        <property name="orderid">
            <column name="orderid" sql-type="int(11)" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>